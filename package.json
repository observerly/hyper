{
  "name": "@observerly/hyper",
  "private": true,
  "version": "0.0.0",
  "description": "Hyper is observerly's Typescript + Vite client for interoperating with the Fiber ASCOM Alpaca API",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/observerly/hyper.git"
  },
  "author": "Micheal J. Roberts",
  "prepublish": "tsc",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/observerly"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/hyper.umd.js",
  "module": "./dist/hyper.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/hyper.es.js",
      "require": "./dist/hyper.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@vitest/coverage-c8": "^0.30.1",
    "c8": "^7.13.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "path": "^0.12.7",
    "prettier": "2.8.4",
    "rollup": "^3.20.2",
    "tslib": "^2.5.0",
    "typescript": "5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add -A ."
    ]
  },
  "dependencies": {
    "ofetch": "^1.0.1"
  }
}
